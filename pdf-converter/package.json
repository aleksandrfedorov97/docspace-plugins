  "license": "Apache-2.0",
  "homepage": "https://github.com/ONLYOFFICE/docspace-plugins/tree/master/pdf-converter",
  "author": "ONLYOFFICE",
  "maintainers": [
    "Ascensio System SIA <integration@onlyoffice.com> (https://www.onlyoffice.com)"
  ],
  "bugs": {
    "email": "support@onlyoffice.com"
  },
  "pluginName": "PDFConverter",
  "logo": "logo.svg",
  "cspDomains": [
    "https://v2.convertapi.com"
  ],
  "main": "index.ts",
  "private": true,
  "scopes": [
    "API",
    "Settings",
    "ContextMenu"
  ],
  "scripts": {
    "build": "webpack && node ./scripts/createZip.js"
  },
  "devDependencies": {
    "jszip": "^3.10.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
